{
  "name": "react-resizable-collapsible-grid",
  "version": "0.0.0-development",
  "description": "",
  "typings": "dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "pnpm -r dev",
    "clean": "pnpm clean:lib && pnpm clean:docs",
    "clean:lib": "rimraf dist",
    "clean:docs": "rimraf docs/dist",
    "build:ts": "tsc",
    "build": " pnpm build:lib && pnpm build:docs",
    "build:css": "postcss   src/**/*.css --dir dist",
    "build:lib": " pnpm build:ts && pnpm build:css",
    "build:docs": "pnpm --filter docs build",
    "build:clean": "pnpm clean && pnpm build:ts && pnpm build:css",
    "watch:css": "postcss  src/**/*.css --dir dist -w",
    "watch:ts:": "tsc -w",
    "watch": "concurrently \"tsc -w\" \"postcss  src/**/*.css --dir dist -w\"  ",
    "docs:publish": "pnpm --filter docs demo",
    "preview": "pnpm --filter docs preview",
    "semantic-release": "semantic-release"
  },
  "keywords": ["react", "resizable", "collapsible","grid"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^7.2.1",
    "cssnano": "^5.1.10",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.1.0",
    "postcss-jit-props": "^1.0.7",
    "postcss-nesting": "^10.1.7",
    "postcss-sorting": "^7.0.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "open-props": "^1.3.16",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philstenning/react-resizable-collapsible-grid.git"
  }
}
