{
  "name": "react-resizable-collapsible-grid",
  "version": "0.0.0-development",
  "description": "Components For Building Resizable Web Apps",
  "typings": "dist/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/css/index.css",
  "files": [
    "/dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "pnpm -r dev",
    "clean": "pnpm clean:lib && pnpm clean:docs",
    "clean:lib": "rimraf dist",
    "clean:docs": "rimraf docs/dist",
    "build:all": "pnpm clean && pnpm build:css && pnpm build:rollup && pnpm build:docs",
    "build:css": "postcss  src/index.css --base src --dir dist/css",
    "build:lib": "tsc",
    "build:docs": "pnpm --filter docs build",
    "build:clean": "pnpm clean && pnpm build:css && pnpm build:lib",
    "lint": "eslint src/**/**.ts* --fix ",
    "lint:docs": "pnpm --filter docs lint",
    "stylelint": "stylelint src/**/*.css --fix",
    "stylelint:docs": "stylelint docs/src/**/*.css --fix",
    "watch:css": "postcss  src/**/*.css --base src --dir dist -w",
    "watch-ts:": "pnpm tsc -w",
    "watch": "concurrently \"pnpm:watch:css\" \"tsc -w\" ",
    "docs:publish": "pnpm --filter docs demo",
    "preview": "pnpm --filter docs preview",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "build:rollup": "rollup -c",
    "rollup:w": "rollup -c --watch"
  },
  "keywords": [
    "react",
    "resizable",
    "collapsible",
    "grid"
  ],
  "author": "Phil Stenning",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "concurrently": "^7.2.2",
    "cssnano": "^5.1.12",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.8",
    "postcss-sorting": "^7.0.1",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philstenning/react-resizable-collapsible-grid.git"
  }
}

